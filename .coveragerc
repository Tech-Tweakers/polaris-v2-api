[run]
source = polaris_api
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    def .*_mock.*:
    def .*_test.*:
    def test_.*:
    class Test.*:
    class .*Test.*:

[html]
directory = coverage_html
title = Polaris API Test Coverage
